{
    "extends": [
        "airbnb"
    ],
    "env": {
        "es6": true,
        "browser": true
    },
    "rules": {
        "comma-dangle": [
            "error",
            {
                "arrays": "always-multiline",
                "objects": "always-multiline",
                "imports": "always-multiline",
                "exports": "always-multiline",
                "functions": "ignore"
            }
        ]
    },
    "settings": {
        "import/core-modules": [ "Dimensions" ]
    }
}